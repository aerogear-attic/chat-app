{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "**/graphql.d.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "interface-over-type-literal": false,
        "curly": [true, "ignore-same-line"],
        "ordered-imports": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}