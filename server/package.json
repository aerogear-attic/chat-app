{
  "name": "chatapp-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prestart": "yarn codegen",
    "start": "ts-node index.ts",
    "test": "jest --runInBand --forceExit",
    "format": "prettier \"**/*.ts\" --write",
    "codegen": "gql-gen",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint **/*.ts -c tslint.json",
    "build": "tsc"
  },
  "jest-junit": {
    "outputDirectory": "./test-results"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/typescript": "^1.3.1",
    "@graphql-codegen/typescript-resolvers": "^1.3.1",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/graphql": "^14.2.2",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.2",
    "@types/pg": "^7.4.14",
    "apollo-server-testing": "^2.6.8",
    "jest": "^24.8.0",
    "jest-junit": "6.4.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.8",
    "@graphql-modules/di": "^0.7.8",
    "@safe-api/middleware": "^0.0.2",
    "apollo-datasource-rest": "^0.6.1",
    "apollo-server-express": "^2.6.8",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-postgres-subscriptions": "^1.0.5",
    "graphql-schema": "^0.5.1",
    "graphql-tools": "^4.0.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "sql-template-strings": "^2.2.2",
    "tslint": "^5.18.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
